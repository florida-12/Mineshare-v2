                        <div class="filter-peculiarities" onclick="openFilter('filter-peculiarities')">
                            <a id="filter-peculiarities" class="dropdown-trigger"
                                data-target="dropdown-filter-peculiarities">Атрибуты</a>
                        </div>
                    </div>
                    <div class="col l2 m4 s6">
                        <form id="form_online" action="" method="get"><input id="get_online" name="online"
                                class="hidden">
                            <ul id="dropdown-filter-online" class="dropdown-content">
                                <li
                                    onclick="document.getElementById('get_online').value = 'Больше';document.getElementById('form_online').submit();">
                                    <a>Больше</a>
                                </li>
                                <li
                                    onclick="document.getElementById('get_online').value = 'Меньше';document.getElementById('form_online').submit();">
                                    <a>Меньше</a>
                                </li>
                            </ul>
                        </form>
                        <div class="filter-online" onclick="openFilter('filter-online')">
                            <a id="filter-online" class="dropdown-trigger"
                                data-target="dropdown-filter-online">Онлайн</a>
                        </div>
                    </div>
                    <div class="col l2 m4 s6">
                        <form id="form_rate" action="" method="get"><input id="get_rate" name="rate" class="hidden">
                            <ul id="dropdown-filter-rating" class="dropdown-content">
                                <li
                                    onclick="document.getElementById('get_rate').value = 'Больше';document.getElementById('form_rate').submit();">
                                    <a>Больше</a>
                                </li>
                                <li
                                    onclick="document.getElementById('get_rate').value = 'Меньше';document.getElementById('form_rate').submit();">
                                    <a>Меньше</a>
                                </li>
                            </ul>
                        </form>
                        <div class="filter-rating" onclick="openFilter('filter-rating')">
                            <a id="filter-rating" class="dropdown-trigger"
                                data-target="dropdown-filter-rating">Рейтинг</a>
                        </div>
                    </div>
                    <div class="col l2 m4 s6">
                        <form id="form_date" action="" method="get"><input id="get_date" name="date" class="hidden">
                            <ul id="dropdown-filter-date" class="dropdown-content">
                                <li
                                    onclick="document.getElementById('get_date').value = 'Новые';document.getElementById('form_date').submit();">
                                    <a>Новые</a>
                                </li>
                                <li
                                    onclick="document.getElementById('get_date').value = 'Старые';document.getElementById('form_date').submit();">
                                    <a>Старые</a>
                                </li>
                            </ul>
                        </form>
                        <div class="filter-date" onclick="openFilter('filter-date')">
                            <a id="filter-date" class="dropdown-trigger" data-target="dropdown-filter-date">Дата</a>
                        </div>
                    </div>

                </div>
            </div>

            <div class="page container">
                <% if (servers.length> 0) { %>
                    <% servers.forEach(server=> { %>
                        <div class="row server-support"
                            onmouseover="hoverServer('#server-rating-<%= server.id %>', '#server-chart-<%= server.id %>', '#server-verification-<%= server.id %>')"
                            onmouseleave="unhoverServer('#server-rating-<%= server.id %>', '#server-chart-<%= server.id %>', '#server-verification-<%= server.id %>')">
                            <div id="server-rating-<%= server.id %>" class="server-rating">
                                <div class="server-rating-tip"></div><img class="server-rating-icon"
                                    src="/img/star.svg">
                                <%= server.rate %>
                            </div>
                            <% if (server.verification) { %>
                                <div id="server-verification-<%= server.id %>" class="server-verification">
                                    <img class="server-verification-icon" src="/img/verification.svg">
                                </div>
                                <% } %>
                                    <% if (server.rate> server.rate_old) { %>
                                        <div id="server-chart-<%= server.id %>" class="server-chart">
                                            <img class="server-chart-icon" src="/img/rate-chart-true.svg">
                                        </div>
                                        <% } else if (server.rate < server.rate_old) { %>
                                            <div id="server-chart-<%= server.id %>" class="server-chart">
                                                <img class="server-chart-icon" src="/img/rate-chart-false.svg">
                                            </div>
                                            <% } %>
                                                <% if (server.boost_status) { %>
                                                  <!--  <div
                                                        style="position: absolute; top: -12.5px; left: 50%; background: #0006b1; border-radius: 6px 6px 0 0; color: #fff; font-family: 'TT Norms Pro', sans-serif; font-size: 12px; font-weight: 500; transform: translate(-50%, -50%); padding-top: 3px; padding-left: 15px; padding-right: 10px; padding-bottom: 4px; user-select: none; animation: horizontal-shaking infinite 0.7s;">
                                                        Грозовая туча<i style="font-size: 10.5px; margin-left: 5px;"
                                                            class="fa-solid fa-cloud-bolt"></i></div> -->
                                                    <% } %>
                                                        <div class="server" <% if (server.premium_status) { %>type="<%= server.premium_color %>"<% } %>>
                                                                            <% if (server.premium_status) { %>
                                                                                <div
                                                                                    class="gradient-<%= server.premium_color %>">
                                                                                </div>
                                                                                <% } %>
                                                                                    <div
                                                                                        class="col l4 s12 server-plate-one">
                                                                                        <div class="server-module"
                                                                                            onclick="window.location = '/server/<%= server.id %>'">
                                                                                            <p class="server-title">
                                                                                                <%= server.title.substring(0,
                                                                                                    35) %>
                                                                                                    <% if
                                                                                                        (server.title.length>
                                                                                                        35) {
                                                                                                        %>..<% } %>
                                                                                            </p>
                                                                                            <p
                                                                                                class="server-description text-color-gray">
                                                                                                <%= server.description.substring(0,
                                                                                                    110) %>..
                                                                                            </p>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div
                                                                                        class="col l4 s12 server-plate-two hide-on-med-and-down">
                                                                                        <% if
                                                                                            (server.banner=='default-server-picture.gif'
                                                                                            ) { %>
                                                                                            <div <% if (server.premium_color == 'andromeda' || server.premium_color == 'western') { %>style="box-shadow: none; border: 3px solid #fff; background-image: url('../img/default-server-picture.gif');"<% } else {%>style="background-image: url('../img/default-server-picture.gif');"<% } %>>
                                                                                            </div>
                                                                                            <% } else { %>
                                                                                                <div <% if (server.premium_color == 'andromeda' || server.premium_color == 'western') { %>style="box-shadow: none; border: 3px solid #fff; background-image: url('../media/pictures/<%= server.banner %>');"<% } else {%>style="background-image: url('../media/pictures/<%= server.banner %>');"<% } %>>
                                                                                                </div>
                                                                                                <% } %>
                                                                                    </div>
                                                                                    <div
                                                                                        class="col l4 s12 server-plate-three">
                                                                                        <div class="row">
                                                                                            <div class="col l2 m2 s2 server-online"
                                                                                                style="padding-top: 8px;">
                                                                                                <% if (server.premium_color == 'andromeda') { %>
                                                                                                    <i class="fa-solid fa-user-astronaut" style="font-size: 20px;"></i>
                                                                                                <% } else if (server.premium_color == 'western') { %>
                                                                                                    <i class="fa-solid fa-user-cowboy" style="font-size: 20px;"></i>
                                                                                                <% } else { %>
                                                                                                    <i class="fa-solid fa-user-group"></i>
                                                                                                <% } %>
                                                                                                <p><%= server.current_players %>
                                                                                                </p>
                                                                                            </div>
                                                                                            <div
                                                                                                class="col l10 m9 s10 server-plate-three-support">
                                                                                                <div class="row">
                                                                                                    <% if
                                                                                                        (server.launcher)
                                                                                                        { %>
                                                                                                        <div class="col l12 s12 server-ip"
                                                                                                            onclick="getIP('{{ server.website }}')"
                                                                                                            style="cursor:pointer">
                                                                                                            <h6
                                                                                                                class="center">
                                                                                                                Лаунчер
                                                                                                            </h6>
                                                                                                        </div>
                                                                                                        <% } else { %>
                                                                                                            <% if
                                                                                                                (server.port=="25565"
                                                                                                                ) { %>
                                                                                                                <div class="col l12 s12 server-ip"
                                                                                                                    onclick="copyIP('#<%= server.id %>')"
                                                                                                                    style="cursor:pointer;">
                                                                                                                    <h6 id="<%= server.id %>"
                                                                                                                        class="center">
                                                                                                                        <%= server.ip
                                                                                                                            %>
                                                                                                                    </h6>
                                                                                                                </div>
                                                                                                                <% } else
                                                                                                                    { %>
                                                                                                                    <div class="col l12 s12 server-ip"
                                                                                                                        onclick="copyIP('#<%= server.id %>')"
                                                                                                                        style="cursor:pointer;">
                                                                                                                        <h6 id="<%= server.id %>"
                                                                                                                            class="center">
                                                                                                                            <%= server.ip
                                                                                                                                %>:<%= server.port %>
                                                                                                                        </h6>
                                                                                                                    </div>
                                                                                                                    <% }}
                                                                                                                        %>
                                                                                                                        <div
                                                                                                                            class="col l12 m12 s12 server-line">
                                                                                                                            <% if
                                                                                                                                (server.premium_status)
                                                                                                                                {
                                                                                                                                %>
                                                                                                                                <% if
                                                                                                                                    (server.rate>
                                                                                                                                    100)
                                                                                                                                    {
                                                                                                                                    %>
                                                                                                                                    <div class="server-line-<%= server.premium_color %>"
                                                                                                                                        style="animation:server-line-animation;animation-duration:1.5s;width:100%;">
                                                                                                                                        <style>
                                                                                                                                            @keyframes server-line-animation {
                                                                                                                                                0% {
                                                                                                                                                    width: 0%
                                                                                                                                                }

                                                                                                                                                100% {
                                                                                                                                                    width: 100%;
                                                                                                                                                }
                                                                                                                                            }
                                                                                                                                        </style>
                                                                                                                                    </div>
                                                                                                                                    <% } else
                                                                                                                                        {
                                                                                                                                        %>
                                                                                                                                        <div class="server-line-<%= server.premium_color %>"
                                                                                                                                            style="animation:server-line-animation;animation-duration:2.5s; width:100%;">
                                                                                                                                            <style>
                                                                                                                                                @keyframes server-line-animation {
                                                                                                                                                    0% {
                                                                                                                                                        width: 0%
                                                                                                                                                    }

                                                                                                                                                    100% {
                                                                                                                                                        width: 100%;
                                                                                                                                                    }
                                                                                                                                                }
                                                                                                                                            </style>
                                                                                                                                        </div>
                                                                                                                                        <% }
                                                                                                                                            %>
                                                                                                                                            <% } else
                                                                                                                                                {
                                                                                                                                                %>
                                                                                                                                                <% if
                                                                                                                                                    (server.rate>
                                                                                                                                                    100)
                                                                                                                                                    {
                                                                                                                                                    %>
                                                                                                                                                    <div class="server-line-gray"
                                                                                                                                                        style="width:100%;">
                                                                                                                                                        <style>
                                                                                                                                                            @keyframes server-line-animation {
                                                                                                                                                                0% {
                                                                                                                                                                    width: 0%
                                                                                                                                                                }

                                                                                                                                                                100% {
                                                                                                                                                                    width: 100%;
                                                                                                                                                                }
                                                                                                                                                            }
                                                                                                                                                        </style>
                                                                                                                                                    </div>
                                                                                                                                                    <% } else
                                                                                                                                                        {
                                                                                                                                                        %>
                                                                                                                                                        <div class="server-line-gray"
                                                                                                                                                            style="animation:server-line-animation;animation-duration:2.5s; width:100%;">
                                                                                                                                                            <style>
                                                                                                                                                                @keyframes server-line-animation {
                                                                                                                                                                    0% {
                                                                                                                                                                        width: 0%
                                                                                                                                                                    }

                                                                                                                                                                    100% {
                                                                                                                                                                        width: 100%;
                                                                                                                                                                    }
                                                                                                                                                                }
                                                                                                                                                            </style>
                                                                                                                                                        </div>
                                                                                                                                                        <% }
                                                                                                                                                            %>
                                                                                                                                                            <% }
                                                                                                                                                                %>
                                                                                                                        </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                        </div>
                        </div>

                        <% }) %>
                            <% } else { %>
                                <div class="server-container-servers-empty">
                                    <img class="privatearea-container-panel-servers-picture-empty"
                                        src="/img/cloud-empty.svg">Ничего
                                    нет!
                                </div>
                                <% } %>
            </div>

            <div class="container center">
                <div class="pagination-pages">
                    <form id="page_form" action="" method="get">
                        <input id="page" name="page" style="visibility: hidden">
                        <a class="link active"
                            onclick="document.getElementById('page').value='1';document.getElementById('page_form').submit();">1</a>
                        <a class="link"
                            onclick="document.getElementById('page').value='2';document.getElementById('page_form').submit();">2</a>
                        <a class="link"
                            onclick="document.getElementById('page').value='3';document.getElementById('page_form').submit();">3</a>
                    </form>
                </div>
            </div>
            <%- footer %>

                <script type="text/javascript">
                    function randomServer() {
                        $('#random').attr('disabled', true);
                        $('#random').html(`
                        <div class="button-random-loader"><div class="progress"><div class="indeterminate"></div></div></div>
                        `)
                        setTimeout(function () {
                            window.location = '/random';
                        }, 100);
                        setTimeout(function () {
                            $('#random').html(`Случайный
                                    сервер<i class="fa-solid fa-arrow-right-from-bracket"></i>`);
                        }, 2000);
                        $('#random').attr('disabled', false);
                    }

                    document.addEventListener('DOMContentLoaded', function () {
                        var loginValue = (new URL(window.location.href)).searchParams.get('login');
                        console.log(loginValue);

                        if (loginValue == 'login') {
                            var elems = document.querySelectorAll('.auth');
                            var instances = M.Modal.init(elems);
                            instances.forEach(element => {
                                element.open();
                            });
                        } else if (loginValue == 'confirmed') {
                            $('#email_confirmation').css('display', 'flex');

                            var elems = document.querySelectorAll('.auth');
                            var instances = M.Modal.init(elems);
                            instances.forEach(element => {
                                element.open();
                            });
                        } else if (loginValue == 'confirmation') {
                            globalError('Подтвердите ваш электронный адрес');
                        }
                    });
                </script>
</body>

</html>